{
  "name": "hufghani-website",
  "version": "1.0.0",
  "private": true,
  "description": "hufghani-website",
  "author": "Hamza Ghani",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-path .gitignore --ext .ts,.tsx,.js"
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^2.0.1",
    "@chakra-ui/icons": "^1.0.15",
    "@chakra-ui/react": "^1.6.8",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@nivo/calendar": "^0.73.0",
    "@nivo/core": "^0.73.0",
    "@nivo/pie": "^0.73.0",
    "@paralleldrive/feature-toggles": "^1.0.4",
    "@paralleldrive/react-feature-toggles": "^2.3.1",
    "@types/jest": "^27.0.2",
    "axios": "^0.22.0",
    "date-fns": "^2.24.0",
    "framer-motion": "^4.1.17",
    "gatsby": "^3.14.2",
    "gatsby-image": "^3.11.0",
    "gatsby-plugin-brotli": "^2.0.0",
    "gatsby-plugin-hotjar": "^1.1.1",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-minify-html": "^1.0.4",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-robots-txt": "^1.6.10",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-lazy-load": "^1.0.2",
    "gatsby-remark-smartypants": "^4.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "identity-obj-proxy": "^3.0.0",
    "lazysizes": "^5.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "url-parse": "^1.5.3"
  },
  "devDependencies": {
    "@loadable/component": "^5.15.0",
    "@reach/router": "^1.3.4",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@types/loadable__component": "^5.13.4",
    "@types/mdx-js__react": "^1.5.4",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-helmet": "^6.1.2",
    "@types/url-parse": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-jest": "^27.2.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gatsby-plugin-sitemap": "^4.10.0",
    "jest": "^27.2.2",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js",
      "^.+\\.[jt]sx?$": "<rootDir>/jest-preprocess.js"
    },
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache",
      "public",
      "e2e-tests"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ]
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    ">0.2%",
    "not IE 11",
    "not IE_Mob 11"
  ]
}
